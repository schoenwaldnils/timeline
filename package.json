  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged && npm run lint:types"
    }
  },
  "lint-staged": {
    "**/*.(ts|tsx)": [
      "eslint --fix",
      "stylelint --formatter verbose"
    ]
  },
  "snyk": true
