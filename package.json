    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "graphql": "^15.5.0",
    "graphql-hooks-memcache": "^2.1.2",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.5",
