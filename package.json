    "@svgr/webpack": "5.5.0",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "accept-language": "3.0.18",
    "algoliasearch": "4.9.0",
    "array-sort": "1.0.0",
    "color": "3.1.3",
    "cookie": "0.4.1",
    "cross-env": "7.0.3",
    "cross-fetch": "3.1.4",
    "eslint-plugin-prettier": "3.4.0",
    "graphql": "15.5.0",
    "graphql-hooks-memcache": "2.1.1",
    "graphql-request": "3.4.0",
