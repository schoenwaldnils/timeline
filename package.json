{
  "name": "timeline",
  "version": "3.2.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "export": "NODE_ENV=production next build && next export",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint '**/*.tsx'",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts-fix": "eslint . --ext .ts,.tsx --fix",
    "lint:types": "tsc",
    "lint:html": "htmlhint out",
    "gen:types-contentful": "graphql-codegen --config codegen.yml -r dotenv-flow/config",
    "favicons-generate": "real-favicon generate faviconDescription.json faviconData.json public/assets/images/favicons",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "snapshot": "build-storybook && percy-storybook --widths=320 -t=500",
    "analyze": "cross-env ANALYZE=true next build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [],
  "repository": "github:schoenwaldnils/timeline",
  "author": {
    "name": "Nils Sch√∂nwald",
    "email": "nils@schoen.world"
  },
  "license": "ISC",
