{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [ "transform-define", "./env-config.js" ],
        "transform-es2015-modules-commonjs"
      ]
    },
    "production": {
      "presets": [
        ["next/babel", {
          "preset-env": { "modules": "commonjs" }
        }]
      ],
      "plugins": [
        [ "transform-define", "./env-config.js" ]
      ]
    }
  }
}
